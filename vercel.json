{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/$1"
      }
    ],
    "builds": [
      {
        "src": "frontend/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ]
  }
  
